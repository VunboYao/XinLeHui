<!--pages/my/my.wxml-->
<view class='container'>
  <view class='my-container'>
    <view class='userinfo'>
      <image class='userinfo-avatar' src='{{userInfo.avatarUrl}}'></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
  </view>
  <view class='float-box'>
    <view class="weui-grids">
        <navigator url="./../test/test" class="weui-grid" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="../../static/02.png" />
          <view class="weui-grid__label">订单</view>
        </navigator>
        <navigator url="" class="weui-grid" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="../../static/02.png" />
          <view class="weui-grid__label">待评价</view>
        </navigator>
        <navigator url="./../my_vip/my_vip" class="weui-grid" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="../../static/02.png" />
          <view class="weui-grid__label">VIP</view>
        </navigator>
    </view>
  </view>
  <view class='service'>
    <view class="weui-grids">
      <block wx:for="{{grids}}" wx:key="*this">
        <navigator url="./../evaluation/evaluation" class="weui-grid" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="/static/02.png" />
          <view class="weui-grid__label">待评价</view>
        </navigator>
      </block>
    </view>
  </view>
  <view class="weui-cells weui-cells_after-title" style='width:100%;margin-top:30rpx;'>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/static/02.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">联系我们</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="./../publish/publish" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/static/02.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">发起救助</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="./../publish_list/publish_list" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="/static/02.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">我的发起</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>
</view>
